package Clase8;
import java.util.ArrayDeque;
import java.util.Queue;


public class Colas {
    public static void main(String[] args) {

        Queue<Integer> cola = new ArrayDeque<>();

        cola.add(5);
        cola.add(10);
        cola.add(3);
        cola.add(20);

        System.out.println(cola);
        System.out.println(cola.element());
    
    }
}